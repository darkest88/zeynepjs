{
  "name": "zeynepjs",
  "version": "2.0.1",
  "description": "a lightweight, multi-level jQuery side menu plugin",
  "main": "./dist/zeynep.js",
  "style": "./dist/zeynep.css",
  "directories": {
    "example": "examples"
  },
  "scripts": {
    "start": "gulp",
    "build": "gulp build",
    "lint": "npm run lint:standard",
    "lint:standard": "standard --verbose | snazzy || exit 0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hsynlms/zeynepjs.git"
  },
  "keywords": [
    "jquery",
    "navigation",
    "menu",
    "side-menu",
    "multi-level-menu"
  ],
  "author": "Huseyin ELMAS",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/hsynlms/zeynepjs/issues"
  },
  "homepage": "https://github.com/hsynlms/zeynepjs#readme",
  "devDependencies": {
    "autoprefixer": "^9.8.6",
    "chalk": "^4.1.0",
    "del": "^5.1.0",
    "gulp": "^4.0.2",
    "gulp-clean-css": "^4.3.0",
    "gulp-header": "^2.0.9",
    "gulp-postcss": "^8.0.0",
    "gulp-rename": "^2.0.0",
    "gulp-sourcemaps": "^2.6.5",
    "gulp-uglify": "^3.0.2",
    "postcss-discard-comments": "^4.0.2",
    "postcss-sorting": "^5.0.1",
    "snazzy": "^8.0.0",
    "standard": "^14.3.4"
  },
  "standard": {
    "ignore": [
      "/dist/",
      "/examples/"
    ]
  }
}
